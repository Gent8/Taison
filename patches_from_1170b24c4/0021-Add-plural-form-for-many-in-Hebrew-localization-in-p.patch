 1 file changed, 2 insertions(+), 1 deletion(-)

diff --git a/i18n/src/commonMain/moko-resources/he/plurals.xml b/i18n/src/commonMain/moko-resources/he/plurals.xml
index 03bd26a33..bb93fe12a 100644
--- a/i18n/src/commonMain/moko-resources/he/plurals.xml
+++ b/i18n/src/commonMain/moko-resources/he/plurals.xml
@@ -99,6 +99,7 @@
     <plurals name="upcoming_relative_time">
         <item quantity="one">מחר</item>
         <item quantity="two">מחרותיים</item>
+        <item quantity="many">בעוד %1$d ימים</item>
         <item quantity="other">בעוד %1$d ימים</item>
     </plurals>
-</resources>
\ No newline at end of file
+</resources>
